generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  name       String   @db.VarChar(50)
  password   String
  dateJoined DateTime @default(now())
  username   String   @db.VarChar(20)
}
